{
	"productName": "vsh",
	"version": "0.0.0",
	"identifier": "org.tellyfin.vsh",
	"build": {
		"beforeDevCommand": "yarn dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "yarn build",
		"frontendDist": "../dist"
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {},
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"title": "vsh",
				"width": 800,
				"height": 600,
				"fullscreen": true,
				"transparent": true,
				"resizable": false
			}
		]
	}
}